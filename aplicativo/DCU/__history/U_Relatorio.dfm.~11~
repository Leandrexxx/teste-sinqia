  object qryRel: TADOQuery
    Active = True
    Connection = dmConexao.conexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'Select cli.CODIGO_CLIENTE,cli.CGC_CPF_CLIENTE,cli.nome as nome_c' +
        'liente,cli.telefone,cli.endereco,cli.bairro,cli.cep,cid.nome as ' +
        'nome_cidade from clientes cli'
      'inner join cidades cid on cid.CODIGO_CIDADE = cli.CODIGO_CIDADE'
      'Where 1 = 1')
    Left = 504
    Top = 48
    object qryRelCODIGO_CLIENTE: TAutoIncField
      FieldName = 'CODIGO_CLIENTE'
      ReadOnly = True
    end
    object qryRelCGC_CPF_CLIENTE: TStringField
      FieldName = 'CGC_CPF_CLIENTE'
      Size = 14
    end
    object qryRelnome_cliente: TStringField
      FieldName = 'nome_cliente'
      Size = 100
    end
    object qryReltelefone: TStringField
      FieldName = 'telefone'
      Size = 14
    end
    object qryRelendereco: TStringField
      FieldName = 'endereco'
      Size = 100
    end
    object qryRelbairro: TStringField
      FieldName = 'bairro'
      Size = 50
    end
    object qryRelcep: TStringField
      FieldName = 'cep'
      Size = 8
    end
    object qryRelnome_cidade: TStringField
      FieldName = 'nome_cidade'
      Size = 100
    end
  end
  object dsRel: TDataSource
    DataSet = qryRel
